    <ContentView.Content>
        <ScrollView>
            <StackLayout VerticalOptions="FillAndExpand" Orientation="Vertical" Spacing="1" x:Name="imgStack">
                <local:CustomWebView x:Name="imgWebView" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" />
            </StackLayout>
        </ScrollView>
    </ContentView.Content>
</ContentView>